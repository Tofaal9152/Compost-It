{
  "expo": {
    "name": "Compost It",
    "slug": "compost-it",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/branding/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/branding/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tofaal9152.compostit"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/branding/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tofaal9152.compostit",
      "edgetoedgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/branding/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@maplibre/maplibre-react-native",
      "expo-maps"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "316d3300-a0bc-4105-b1be-34423e9e5c42"
      }
    },
    "owner": "tofaal9152"
  }
}
